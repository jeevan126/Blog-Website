{
    "builds": [
        {
            "src": "mysite/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" },
            "env": {
                "PIP_DISABLE_PIP_VERSION_CHECK": "1"
              }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mysite/wsgi.py"
        }
    ]
}